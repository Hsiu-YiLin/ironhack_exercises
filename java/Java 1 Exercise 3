//Java 1 Exercise 3
var country;

country = {
    name: "Uruguay",
    continent: "South America",
    capital: "Montevideo"
};

function objectToArray(country) {
    var array= [];
    for (i in country) {
        array.push(country[i]);
    }
    return array;
}

// Should print [ "Uruguay", "South America", "Montevideo" ]
console.log(objectToArray(country));